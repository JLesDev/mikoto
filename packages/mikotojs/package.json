{
  "name": "mikotojs",
  "version": "0.0.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "generate:server": "sophon generate ./schema.sophon -t ts-socketio-server -o ../../apps/server/src/services/schema.ts",
    "generate:client": "sophon generate ./schema.sophon -t ts-socketio-client -o ./src/schema.ts",
    "generate": "npm run generate:server && npm run generate:client",
    "typecheck": "tsc --noEmit",
    "build": "tsc"
  },
  "devDependencies": {
    "@sophon-js/cli": "^0.0.4",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "socket.io-client": "^4.5.1",
    "typed-emitter": "^2.1.0"
  }
}
