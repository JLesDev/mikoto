    "openapi3-ts": "^4.4.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@hyperschema/client": "^0.0.4",
    "@mikoto-io/permcheck": "0.0.1",
    "@zodios/core": "^10.9.6",
    "@zodios/plugins": "^10.6.0",
    "axios": "^1.6.7",
    "events": "^3.3.0",
    "isomorphic-ws": "^5.0.0",
    "mobx": "^6.10.0",
    "openapi-fetch": "^0.12.0",
    "socket.io-client": "^4.5.1",
    "tsx": "^4.19.1",
